<Window x:Class="CTP.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vms="clr-namespace:CTP.core"
        xmlns:lvc="clr-namespace:LiveChartsCore.SkiaSharpView.WPF;assembly=LiveChartsCore.SkiaSharpView.WPF"
        Title="MainWindow" Height="450" Width="800">
    <Window.DataContext>
        <vms:ViewModel />
    </Window.DataContext>
    <StackPanel>
        <Button x:Name="FilePickerButton" Content="Wybierz plik" Margin="5" Width="100" Click="FilePickerButton_Click"></Button>
        <lvc:CartesianChart
                SyncContext="{Binding Sync}"
                Series="{Binding Series}"
                XAxes="{Binding XAxes}"
                HorizontalAlignment="Center" Height="384" Width="750">
        </lvc:CartesianChart>
    </StackPanel>
</Window>
